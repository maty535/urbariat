---
layout: null
---
[
  {% for post in site.posts %}
    {% capture search_key %} {{post.date | date: "%Y-%m-%d" }} {{ post.title }} {% endcapture %}
    {
      "title": "{{ post.title | escape }}",
      "category": "{{ post.category }}",
      "tags": "{{ post.tags | join: ', ' }}",
      "url": "{{ post.url | prepend: site.baseurl }}"
      "search_key": "{{ search_key | slugify: 'latin' }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
